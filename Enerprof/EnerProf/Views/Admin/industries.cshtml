@model IEnumerable<EnerProf.Models.Industries>

@{
    ViewBag.Title = "industries";
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}

<h2>industries</h2>

<p>
    @Html.ActionLink("Create New", "industry")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "industry", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteIndustry", new { id=item.Id })
        </td>
    </tr>
}

</table>
